@model AgileProject.Models.Teacher

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm("UploadImage", "Teachers", FormMethod.Post,
                                        new { enctype = "multipart/form-data" }))
{
